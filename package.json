{
  "name": "iterable-async",
  "version": "1.0.3",
  "description": "Async methods for looping over an iterable object with the familiar array api.",
  "main": "src",
  "scripts": {
    "test:code-style": "npm run lint:check",
    "test:unit-tests": "./node_modules/.bin/mocha test",
    "test": "npm run test:unit-tests && npm run test:code-style",
    "lint:fix": "./node_modules/.bin/prettier --write 'src/**/*.{js,ts,css,html}' --write 'test/**/*.{js,ts,css,html}' --write 'docs/**/*.{js,ts,css,html}'",
    "lint:check": "./node_modules/.bin/eslint src test && ./node_modules/.bin/eslint --no-eslintrc --env es6,node --rule 'no-unused-var: off' docs",
    "lint": "npm run lint:fix --silent && npm run lint:check",
    "prepublishOnly": "npm ci && npm test",
    "postlint": "echo \"No issues.\"",
    "posttest":"echo \"No issues.\""
  },
  "author": {
    "name": "Sykander Gul",
    "email": "scanda@live.co.uk"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Sykander/iterable-async.git"
  },
  "license": "MIT",
  "devDependencies": {
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "eslint": "^6.6.0",
    "eslint-config-prettier": "^6.5.0",
    "eslint-plugin-prettier": "^3.1.1",
    "faker": "^4.1.0",
    "husky": "^3.1.0",
    "mocha": "^6.2.2",
    "prettier": "^1.19.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint:check"
    }
  },
  "keywords": [
    "promise",
    "iterable",
    "async",
    "array",
    "methods"
  ],
  "runkitExampleFilename": "docs/playground.js"
}
