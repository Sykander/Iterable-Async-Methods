{
    "parserOptions": {
        "ecmaVersion": 2019
    },
    "env": {
        "es6": true,
        "node": true,
        "mocha": true
    },
    "plugins": ["prettier"],
    "extends": ["eslint:recommended", "prettier"],
    "rules": {
        "no-await-in-loop": "error",
        "no-console": "error",
        "array-callback-return": "error",
        "complexity": ["error", 20],
        "prettier/prettier": "error"
    }
}